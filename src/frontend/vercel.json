{
  "version": 2,
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "../../api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ],
  "env": {
    "PYTHONPATH": "../.."
  },
  "functions": {
    "../../api/index.py": {
      "runtime": "@vercel/python",
      "maxDuration": 30,
      "memory": 512
    }
  }
}
