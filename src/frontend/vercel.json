{
  "version": 2,
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/python/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/python/debug.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/python/debug",
      "dest": "api/python/debug.py"
    },
    {
      "src": "/api/python/(.*)",
      "dest": "api/python/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
